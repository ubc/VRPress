{
  "env": {
    "jest": true,
    "node": true
  },
  "extends": ["wordpress"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true,
      "globalReturn": true
    }
  },
  "plugins": ["jsx-a11y"]
}
